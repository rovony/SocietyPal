# .deployignore - DeployHQ Deployment Exclusions
# Exclude files/directories from being deployed to production

# =============================================================================
# ADMIN AND LOCAL FILES (High Priority Exclusions)
# =============================================================================
Admin-Local/
.deployHQ/
RooWork/

# =============================================================================
# DEVELOPMENT AND BUILD FILES
# =============================================================================

# Node.js dependencies (will be built on server)
node_modules/
.npm/
.yarn/
.pnpm-store/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.nyc_output

# Development dependencies (Composer handles production deps)
vendor/

# Package manager lock files (use on build server instead)
# Uncomment if you want to exclude these:
# package-lock.json
# yarn.lock
# composer.lock

# =============================================================================
# ENVIRONMENT AND CONFIGURATION
# =============================================================================

# Environment files (managed separately on server)
.env
.env.*
!.env.example

# IDE and Editor files
.vscode/
.idea/
.phpstorm.meta.php
*.swp
*.swo
*~

# =============================================================================
# VERSION CONTROL AND CI/CD
# =============================================================================

# Git files
.git/
.gitignore
.gitattributes

# GitHub workflows (not needed on production)
.github/

# CI/CD files
.gitlab-ci.yml
.travis.yml
.circleci/
phpunit.xml

# =============================================================================
# DOCUMENTATION AND README FILES
# =============================================================================

# Documentation files
README.md
CHANGELOG.md
CONTRIBUTING.md
LICENSE
LICENSE.md
*.md
docs/
documentation/

# =============================================================================
# TESTING AND QUALITY ASSURANCE
# =============================================================================

# Test files
tests/
Tests/
spec/
.phpunit.result.cache
phpunit.xml
.phpunit.cache/

# Code quality tools
.php_cs.cache
.php-cs-fixer.cache
phpcs.xml
phpmd.xml
.scrutinizer.yml

# =============================================================================
# LOGS AND TEMPORARY FILES
# =============================================================================

# Log files
*.log
logs/
storage/logs/*.log

# Temporary files
tmp/
temp/
.tmp/
.cache/

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# =============================================================================
# SECURITY SENSITIVE FILES
# =============================================================================

# SSH keys and certificates
*.pem
*.key
*.crt
*.csr
id_rsa*
known_hosts

# Backup files
*.bak
*.backup
*.sql
*.dump

# =============================================================================
# LARAVEL SPECIFIC EXCLUSIONS
# =============================================================================

# Laravel storage (managed separately)
storage/app/
storage/framework/cache/
storage/framework/sessions/
storage/framework/testing/
storage/framework/views/
storage/logs/

# Laravel compiled assets (will be built on server)
public/build/
public/hot
public/storage

# Laravel configuration cache (generated on server)
bootstrap/cache/*.php

# =============================================================================
# CODECANYON / SCRIPT SPECIFIC
# =============================================================================

# CodeCanyon management files
codecanyon_management/
installer_backup/
update_tracking/

# Vendor downloads and backups
vendor_downloads/
OrigCC-Backups-zaj/

# =============================================================================
# HOSTINGER SPECIFIC EXCLUSIONS
# =============================================================================

# Hostinger specific files
.well-known/
cgi-bin/
error_log
access_log

# =============================================================================
# DEPLOYMENT UTILITIES
# =============================================================================

# Deployment scripts (run locally, not on server)
deploy.sh
deploy/
deployment/
scripts/deploy*

# Build tools configuration
webpack.mix.js
vite.config.js
rollup.config.js
gulpfile.js
Gruntfile.js

# =============================================================================
# MISCELLANEOUS
# =============================================================================

# Archive files
*.zip
*.tar
*.tar.gz
*.rar
*.7z

# Image optimization files
*.psd
*.ai
*.sketch

# Database files
*.sqlite
*.db

# Sass cache
.sass-cache/

# =============================================================================
# CUSTOM PROJECT EXCLUSIONS
# =============================================================================

# Add your project-specific exclusions here
# Example:
# custom-scripts/
# internal-docs/
# development-assets/
